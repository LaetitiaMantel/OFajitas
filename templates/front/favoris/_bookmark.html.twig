<form action="
	    {% if app.session.get('favoris')[key] is defined %}
	        {{ path('front_favorite_delete', {id: key}) }}
	    {% else %}
	        {{ path('front_favorite_add', {id: key}) }}
	    {% endif %}" method="POST">
	<button class="btn" id="btn" data-product-id="{{ key }}">
		<i class="bi bi-heart-fill position-absolute top-0 start-0 m-3 icon 
		{% if not app.session.get('favoris')[key] is not defined %} icon-plus {% endif %}"></i>
		</i>
	</button>
</form> 