<form method="post" action="{{ path('front_user_delete',{'email': user.email}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer votre compte?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn btn-danger mb-2">Supprimer mon compte</button>
</form>
