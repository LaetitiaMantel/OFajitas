<!DOCTYPE html>

<title>Brand</title>

{% block body %}
    <h1>Brand</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ brand.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ brand.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ brand.slug }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ brand.createdAt ? brand.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ brand.updatedAt ? brand.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_back_brand_index') }}">back to list</a>

    <a href="{{ path('app_back_brand_edit', {'id': brand.id}) }}">edit</a>

    {{ include('back/brand/_delete_form.html.twig') }}
{% endblock %}
