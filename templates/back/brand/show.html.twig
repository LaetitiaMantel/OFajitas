
{% extends 'base.html.twig' %}

{% block title %}
	{{  brand.name }}
{% endblock %}
{% block body %}
	<div class="container">
		<h2 class="h2-color">Marque</h2>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ brand.id }}</td>
				</tr>
				<tr>
					<th>Name</th>
					<td>{{ brand.name }}</td>
				</tr>
				<tr>
					<th>Slug</th>
					<td>{{ brand.slug }}</td>
				</tr>
				<tr>
					<th>CreatedAt</th>
					<td>{{ brand.createdAt ? brand.createdAt|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
				<tr>
					<th>UpdatedAt</th>
					<td>{{ brand.updatedAt ? brand.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
			</tbody>
		</table>

		<a class="btn btn-success mb-2" href="{{ path('back_brand_index') }}">Retour Ã  la liste</a>

		<a class="btn btn-warning mb-2" href="{{ path('back_brand_edit', {'id': brand.id}) }}">Modifier</a>

		{{ include('back/brand/_delete_form.html.twig') }}
	</div>

{% endblock %}
